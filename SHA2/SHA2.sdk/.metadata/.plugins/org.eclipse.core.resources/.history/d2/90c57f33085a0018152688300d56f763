/*
 * helloworld.c: simple test application
 *
 * This application configures UART 16550 to baud rate 9600.
 * PS7 UART (Zynq) is not initialized by this application, since
 * bootrom/bsp configures it to baud rate 115200
 *
 * ------------------------------------------------
 * | UART TYPE   BAUD RATE                        |
 * ------------------------------------------------
 *   uartns550   9600
 *   uartlite    Configurable only in HW design
 *   ps7_uart    115200 (configured by bootrom/bsp)
 */

#include <stdio.h>
#include "platform.h"
#include "xil_printf.h"
#include "mb_interface.h"

#define N 16

int main()
{
	char *chunk = "b109f3bbbc244eb82441917ed06d618b9008dd09b3befd1b5e07394c706a8bb980b1d7785e5976ec049b46df5f1326af5a2ea6d103fd07c95385ffab0cacbc86";
	char *result;
    init_platform();

    xil_printf("\n\rHello Stream Coprocessor\n\r");

    xil_printf("%s\n\r",chunk);
	putfsl(chunk, 0);
	xil_printf("\n\rPUT DONW\n\r");
	getfsl(result, 0);
	while(result){
		xil_printf("%0x\n\r",result);
		&result+=4;

	}


    xil_printf("\n\rDone!");

    cleanup_platform();
    return 0;
}
